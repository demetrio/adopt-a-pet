{
  "name": "adopt-me",
  "version": "1.0.0",
  "description": "A simple React project",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/index.html",
    "dev:mock": "cross-env PET_MOCK=mock npm run dev",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "test": "jest",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "parcel build src/index.html --no-cache --no-source-maps --no-minify"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/react": "^11.2.5",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^4.2.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.3",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@demetrio/pet": "git+https://github.com/demetrio/frontendmasters-pet-api.git",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.1.5",
    "@reach/router": "^1.3.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
